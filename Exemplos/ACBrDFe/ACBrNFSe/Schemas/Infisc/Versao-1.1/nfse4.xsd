				<!-- Situacao -->
				<xs:element maxOccurs="1" minOccurs="1" name="sit" type="TSituacaoNota"/>
				<!-- Motivo  -->
				<xs:element minOccurs="0" name="mot" type="TString256"/>
				<xs:element maxOccurs="1" minOccurs="0" ref="ds:Signature"/>
			</xs:sequence>
			<xs:attribute fixed="1.0" name="versao" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<!-- Definicao da estrutura de dados para Consulta do Status de uma NFSe -->
	<xs:element name="pedConsultaTrans">
		<xs:complexType>
			<xs:sequence>
				<!-- CNPJ de um contribuinte -->
				<xs:element maxOccurs="1" minOccurs="1" name="CNPJ" type="TCnpj"/>
				<!-- Chave de acesso   -->
				<xs:element maxOccurs="1" minOccurs="1" name="chvAcessoNFS-e" type="TChaveAcesso"/>
				<!-- Assinatura digital   -->
				<xs:element maxOccurs="1" minOccurs="0" ref="ds:Signature"/>
			</xs:sequence>
			<xs:attribute fixed="1.0" name="versao" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<!-- Definicao da estrutura de dados para Retorno de Status de uma NFSe -->
	<xs:element name="resConsultaTrans">
		<xs:complexType>
			<xs:sequence>
				<!-- CNPJ de um contribuinte -->
				<xs:element maxOccurs="1" minOccurs="1" name="CNPJ" type="TCnpj"/>
				<!-- Chave de acesso   -->
				<xs:element maxOccurs="1" minOccurs="1" name="chvAcessoNFS-e" type="TChaveAcesso"/>
				<!-- Data e Hora recebimento   -->
				<xs:element maxOccurs="1" minOccurs="1" name="dhRecbto" type="TDataHora"/>
				<!-- Protocolo   -->
				<xs:element maxOccurs="1" minOccurs="1" name="nProt" type="TString15"/>
				<!-- Situacao da NFSe   -->
				<xs:element maxOccurs="1" minOccurs="1" name="sitNFS-e" type="TSituacaoNFSe"/>
				<!-- sit   -->
				<xs:element maxOccurs="1" minOccurs="1" name="sit" type="TSituacaoNota"/>
				<!-- Motivo   -->
				<xs:element maxOccurs="1" minOccurs="0" name="mot" type="TString256"/>
				<!-- Assinatura digital   -->
				<xs:element maxOccurs="1" minOccurs="0" ref="ds:Signature"/>
			</xs:sequence>
			<xs:attribute fixed="1.0" name="versao" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="inutilizacao">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="solicitacaoInutilizacao"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="inutilizacaoRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="solicitacaoInutilizacao"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- Definicao da estrutura de dados para pedido de inutilizacao de numeracao de NFSe -->
	<xs:element name="solicitacaoInutilizacao">
		<xs:complexType>
			<xs:sequence>
				<!-- Numero da Solicitacao -->
				<xs:element maxOccurs="1" minOccurs="1" name="numeroSolicitacao" type="TNum"/>
				<!-- CNPJ contribuinte -->
				<xs:element maxOccurs="1" minOccurs="1" name="cnpjContribuinte" type="TCnpj"/>
				<!-- Data da solicitacao -->
				<xs:element maxOccurs="1" minOccurs="1" name="dataSolicitacao" type="TData"/>
				<!-- Hora da solicitacao -->
				<xs:element maxOccurs="1" minOccurs="1" name="horaSolicitacao" type="THora"/>
				<!-- Conteudo das inutilizacoes -->
				<xs:element maxOccurs="1" minOccurs="1" ref="numerosInutilizados"/>
				<!-- Assinatura digital   -->
				<xs:element maxOccurs="1" minOccurs="0" ref="ds:Signature"/>
			</xs:sequence>
			<xs:attribute fixed="1.0" name="versao" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<!-- Definicao de estrutura para a numeracao e serie a serem inutilizadas -->
	<xs:element name="numerosInutilizados">
		<xs:complexType>
			<xs:sequence>
				<!-- Informacoes sobre uma nota com numeracao inutilizada -->
				<xs:element maxOccurs="999" minOccurs="1" ref="inutilizar"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- Definicao de estrutura para a numeracao e serie a serem inutilizadas -->
	<xs:element name="inutilizar">
		<xs:complexType>
			<xs:attribute name="numero" type="xs:string" use="required"/>
			<xs:attribute name="serie" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<!-- Definicao da estrutura de dados para pedido de inutilizacao de numeracao de NFSe -->
	<xs:element name="retornoSolicitacaoInutilizacao">
		<xs:complexType>
			<xs:sequence>
				<!-- Numero da Solicitacao -->
				<xs:element maxOccurs="1" minOccurs="1" name="numeroSolicitacao" type="TNum"/>
				<!-- CNPJ contribuinte -->
				<xs:element maxOccurs="1" minOccurs="1" name="cnpjContribuinte" type="TCnpj"/>
				<!-- Data da solicitacao -->
				<xs:element maxOccurs="1" minOccurs="1" name="dataRetornoSolicitacao" type="TData"/>
				<!-- Hora da solicitacao -->
				<xs:element maxOccurs="1" minOccurs="1" name="horaRetornoSolicitacao" type="THora"/>
				<!-- Mensagem -->
				<xs:element maxOccurs="1" minOccurs="1" name="mensagem" type="TString256"/>
				<!-- Protocolos de retorno de inutilizacao -->
				<xs:element maxOccurs="1" minOccurs="0" ref="protocolosInutilizacao"/>
				<!-- Assinatura digital   -->
				<xs:element maxOccurs="1" minOccurs="0" ref="ds:Signature"/>
			</xs:sequence>
			<xs:attribute fixed="1.0" name="versao" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<!-- Definicao de estrutura para o retorno de inutilizacao de numeracao -->
	<xs:element name="protocolosInutilizacao">
		<xs:complexType>
			<xs:sequence>
				<xs:element maxOccurs="999" minOccurs="0" ref="inutilizado"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="inutilizado">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="numero" type="xs:string" use="required"/>
					<xs:attribute name="serie" type="xs:string" use="required"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<!-- ***********************************************************  -->
	<!-- Definicao de estruturas de dados compostas                   -->
	<!-- Definicao da estrutura de dados para uma NFSe -->
	<xs:element name="NFS-e">
		<xs:complexType>
			<xs:sequence>
				<!-- Informacoes sobre uma nota -->
				<xs:element ref="infNFSe"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- Definicao da estrutura de dados para uma CFSe -->
	<xs:element name="CFS-e">
		<xs:complexType>
			<xs:sequence>
				<!-- Informacoes sobre uma nota -->
				<xs:element ref="infCFSe"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- Definicao da estrutura de dados para as informacoes de uma NFSe -->
	<xs:element name="infNFSe">
		<xs:complexType>
			<xs:sequence>
				<!-- Dados de identificacao da NFSe -->
				<xs:element ref="Id"/>
				<!-- Dados do emitente da NFSe -->
				<xs:element ref="prest"/>
				<!-- Dados de tomador da NFSe -->
				<xs:element ref="TomS"/>
				<!-- Dados de intermediario da NFSe -->
				<xs:element maxOccurs="1" minOccurs="0" ref="intermediario"/>
				<!-- Dados do local de entrega  -->
				<xs:element maxOccurs="1" minOccurs="0" ref="dadosDaObra"/>
				<!-- Dados da transportadora  -->
				<xs:element maxOccurs="1" minOccurs="0" ref="transportadora"/>
				<!-- Detalhes dos itens de servico da NFSe -->
				<xs:element maxOccurs="999" minOccurs="0" ref="det"/>
				<!-- Despesas Adicionais nao tributavel NFSe -->
				<xs:element maxOccurs="1" minOccurs="0" ref="despesas"/>
				<!-- Reembolso da NFSe -->
				<xs:element maxOccurs="999" minOccurs="0" ref="reembolso"/>
				<!-- Dados de totalizacao da NFSe -->
				<xs:element ref="total"/>
				<!-- Dados de cobranca da NFSe -->
				<xs:element maxOccurs="1" minOccurs="0" ref="faturas"/>
				<!-- Local de tributacao a ser exibido na area de Informacoes Adicionais -->
				<xs:element maxOccurs="1" minOccurs="0" name="infAdicLT" type="TCodMunIBGE"/>

				<!-- Indica se deve ser exibido Exportacao de Servico na area de Informacoes Adicionais -->
				<xs:element maxOccurs="1" minOccurs="0" name="infAdicES" type="TSimNao"/>
				<!-- Informacoes adicionais de interesse do fisco -->
				<xs:element maxOccurs="999" minOccurs="0" name="infAdic" type="TString256"/>
				<!-- Exibir valor aproximado de tributos nas informacoes adicionais -->
				<xs:element maxOccurs="1" minOccurs="0" name="infAdicAT" type="TChar1e2"/>
			</xs:sequence>
			<!-- Versao do formato XML da NFSe em esta sendo submetido o arquivo XML de lote -->
			<xs:attribute name="versao" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<!-- Definicao da estrutura de dados para as informacoes de um CFSe -->
	<xs:element name="infCFSe">
		<xs:complexType>
			<xs:sequence>
				<!-- Dados de identificacao da NFSe -->
				<xs:element name="Id" type="IdCupom"/>
				<!-- Dados do emitente da NFSe -->
				<xs:element ref="prest"/>
				<!-- Dados de tomador da NFSe -->
				<xs:element name="TomS" type="TomSCupom"/>
				<!-- Detalhes dos itens de servico da NFSe -->
				<xs:element maxOccurs="999" minOccurs="1" name="det" type="detCupom"/>
				<!-- Detalhes do estacionamento -->
				<xs:element maxOccurs="1" minOccurs="0" ref="estacionamento"/>
				<!-- Dados de totalizacao da NFSe -->
				<xs:element name="total" type="totalCupom"/>
			</xs:sequence>
			<!-- Versao do formato XML da NFSe em esta sendo submetido o arquivo XML de lote -->
			<xs:attribute name="versao" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<!-- Definicao da estrutura de dados para a identificacao de uma NFSe -->
	<xs:element name="Id">
		<xs:complexType>
			<xs:sequence>
				<!-- Codigo numerico aleatorio para compor chave de acesso  -->
				<xs:element name="cNFS-e" type="TNumAleatorio"/>
				<!-- Modelo de uma NFSe -->
				<xs:element name="mod" type="TModelo"/>
				<!-- Serie da nota  -->
				<xs:element name="serie" type="TSerie"/>
				<!-- Numero da nota  -->
				<xs:element name="nNFS-e" type="TNF"/>
				<!-- Data de emissao -->
				<xs:element name="dEmi" type="TData"/>
				<!-- Hora de emissao -->
				<xs:element name="hEmi" type="THora"/>
				<!-- Tipo de uma NFSe -->
				<xs:element name="tpNF" type="TNFSe"/>
				<!-- Chave de acesso de uma NFSe -->
				<xs:element name="refNF" type="TChaveAcesso"/>
				<!-- Formato de impressao de uma NFSe: 1-Retrato, 2-Paisagem -->
				<xs:element maxOccurs="1" minOccurs="0" name="tpImp" type="TImpressao"/>
				<!-- Formato de emissao de uma NFSe: N-Normal, C-Contigencia -->
				<xs:element maxOccurs="1" minOccurs="0" name="tpEmis" type="TEmissao"/>
				<!-- NFSe cancelada (S) ou nao (N) -->
				<xs:element maxOccurs="1" minOccurs="0" name="cancelada" type="TSimNao"/>
				<!-- Motivo do cancelamento  -->
				<xs:element maxOccurs="1" minOccurs="0" name="motCanc" type="TMotCanc"/>
				<!-- Data do cancelamento -->
				<xs:element maxOccurs="1" minOccurs="0" name="dataCanc" type="TData"/>
				<!-- NFSe de debito (S) ou nao (N) -->
				<xs:element maxOccurs="1" minOccurs="0" name="notaDebito" type="TSimNao"/>
				<!-- Numero da nota substitutiva da anulada/cancelada -->
				<xs:element maxOccurs="1" minOccurs="0" name="notaSub" type="TNF"/>
				<!-- Incluir canhoto na imagem da nota: 0 nao incluir, 1 incluir no topo, 2 incluir no rodape -->
				<xs:element maxOccurs="1" minOccurs="0" name="canhoto" type="TChar0ate2"/>
				<!-- Ambiente de emissao da NFSe: 1 producao ou 2 homologacao -->
				<xs:element maxOccurs="1" minOccurs="0" name="ambienteEmi" type="TChar1e2"/>
				<!-- Forma de emissao da NFSe: 1 via Portal, 2 via servicos Web, 3 via upload ou 4 via RPS -->
				<xs:element minOccurs="0" name="formaEmi" type="TChar1ate4"/>
				<!-- Nota de empreitada global: 1 caso seja ou 2 caso contrario -->
				<xs:element maxOccurs="1" minOccurs="0" name="empreitadaGlobal" type="TChar1e2"/>
				<!-- Codigo de validacao do RPS (USO INTERNO) -->
				<xs:element maxOccurs="1" minOccurs="0" name="codRPS" type="TString15"/>
				<!-- Numero do RPS papel que originou a nota -->
				<xs:element maxOccurs="1" minOccurs="0" name="rps" type="TNF"/>
				<!-- Serie do RPS papel que originou a nota  -->
				<xs:element maxOccurs="1" minOccurs="0" name="serierps" type="TSerie"/>
				<!-- Data de emissao do RPS papel que originou a nota  -->
				<xs:element maxOccurs="1" minOccurs="0" name="dataEmissaoRpsPapel" type="TData"/>
				<!-- Indica se o sistema deve ou nao enviar um e-mail para o tomador  -->
				<xs:element maxOccurs="1" minOccurs="0" name="enviarEmail" type="TSimNao"/>
				<!-- Chave de Acesso da nota fiscal substituida-->
				<xs:element maxOccurs="1" minOccurs="0" name="chaveAcessoSubstituida" type="TChaveAcesso"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- Definicao da estrutura de dados para a identificacao de um CFSe -->
	<xs:complexType name="IdCupom">
		<xs:sequence>
			<!-- Codigo numerico aleatorio para compor chave de acesso  -->
			<xs:element name="cCFS-e" type="TNumAleatorio"/>
			<!-- Serie da nota  -->
			<xs:element name="serie" type="TSerie"/>
			<!-- Numero da nota  -->
			<xs:element name="nCFS-e" type="TNF"/>
			<!-- Data de emissao -->
			<xs:element name="dEmi" type="TData"/>
			<!-- Hora de emissao -->
			<xs:element name="hEmi" type="THora"/>
			<!-- Chave de acesso de uma NFSe -->
			<xs:element name="refNF" type="TChaveAcesso"/>
			<!-- NFSe cancelada (S) ou nao (N) -->
			<xs:element maxOccurs="1" minOccurs="0" name="cancelada" type="TSimNao"/>
			<!-- Motivo do cancelamento  -->
			<xs:element maxOccurs="1" minOccurs="0" name="motCanc" type="TMotCanc"/>
			<!-- Data do cancelamento -->
			<xs:element maxOccurs="1" minOccurs="0" name="dataCanc" type="TData"/>
			<!-- Ambiente de emissao da NFSe: 1 producao ou 2 homologacao -->
			<xs:element name="ambienteEmi" type="TChar1e2"/>
			<!-- Forma de emissao da NFSe: 1 via Portal, 2 via servicos Web, 3 via upload ou 4 via RPS -->
			<xs:element name="formaEmi" type="TChar1ate4"/>
		</xs:sequence>
	</xs:complexType>
	<!-- Definicao da estrutura de dados para o emitente de uma NFSe e/ou Cupom Fiscal-->
	<xs:element name="prest">
		<xs:complexType>
			<xs:sequence>
				<!-- CNPJ   -->
				<xs:element name="CNPJ" type="TCnpj"/>
				<!-- Nome ou razao social -->
				<xs:element name="xNome" type="TString150"/>
				<!-- Nome fantasia -->
				<xs:element maxOccurs="1" minOccurs="0" name="xFant" type="TString60"/>
				<!-- Inscricao municipal  -->
				<xs:element name="IM" type="TString15"/>
				<!-- endereco de correio eletronico do emitente  -->
				<xs:element maxOccurs="1" minOccurs="0" name="xEmail" type="TString50"/>
				<!-- URL do site do emitente  -->
				<xs:element maxOccurs="1" minOccurs="0" name="xSite" type="TString50"/>
				<!-- Endereco do emitente  -->
				<xs:element ref="end"/>
				<!-- Fone  -->
				<xs:element maxOccurs="1" minOccurs="0" name="fone" type="TFone"/>
				<!-- Fone alternativo -->
				<xs:element maxOccurs="1" minOccurs="0" name="fone2" type="TFone"/>
				<!-- Inscricao Estadual  -->
				<xs:element maxOccurs="1" minOccurs="0" name="IE" type="TString15"/>
				<!-- Regime tributario  -->
				<xs:element name="regimeTrib" type="TChar1ate3"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- Definicao da estrutura de dados para endereco de um emitente de uma NFSe e/ou Cupom Fiscal-->
	<xs:element name="end">
		<xs:complexType>
			<xs:sequence>
				<!-- Logradouro  -->
				<xs:element name="xLgr" type="TString100"/>
				<!-- Numero  -->
				<xs:element name="nro" type="TString6"/>
				<!-- Complemento  -->
				<xs:element maxOccurs="1" minOccurs="0" name="xCpl" type="TString100"/>
				<!-- Bairro  -->
				<xs:element name="xBairro" type="TString100"/>
				<!-- Codigo do municipio -->
				<xs:element name="cMun" type="TCodMunIBGE"/>
				<!-- Nome do municipio -->
				<xs:element name="xMun" type="TString60"/>
				<!-- UF -->
				<xs:element name="UF" type="TUf"/>
				<!-- CEP  -->
				<xs:element name="CEP" type="TCep"/>
				<!-- Codigo do Pais   -->
				<xs:element name="cPais" type="TcPais"/>
				<!-- Pais  -->
				<xs:element name="xPais" type="TString100"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- Definicao da estrutura de dados para tomador de uma NFSe -->
	<xs:element name="TomS">
		<xs:complexType>
			<xs:sequence>
				<xs:choice>
					<!-- CNPJ de um contribuinte -->
					<xs:element name="CNPJ" type="TCnpj"/>
					<!-- CPF de um contribuinte -->
					<xs:element name="CPF" type="TCpf"/>
				</xs:choice>
				<!-- Nome ou razao social -->
				<xs:element maxOccurs="1" minOccurs="0" name="xNome" type="TString100"/>
				<!-- Endereco -->
				<xs:element maxOccurs="1" minOccurs="0" ref="ender"/>
				<!-- Endereco de correio eletronico -->
				<xs:element maxOccurs="1" minOccurs="0" name="xEmail" type="TString120"/>
				<!-- Inscricao Estadual  -->
				<xs:element maxOccurs="1" minOccurs="0" name="IE" type="TString15"/>
				<!-- Inscricao municipal  -->
				<xs:element maxOccurs="1" minOccurs="0" name="IM" type="TString15"/>
				<!-- Inscricao municipal eventual -->
				<xs:element maxOccurs="1" minOccurs="0" name="IME" type="TString15"/>
				<!-- Fone  -->
				<xs:element maxOccurs="1" minOccurs="0" name="fone" type="TFone"/>
				<!-- Fone alternativo  -->
				<xs:element maxOccurs="1" minOccurs="0" name="fone2" type="TFone"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- Definicao da estrutura de dados para Intermediario de uma NFSe -->
	<xs:element name="intermediario">
		<xs:complexType>
			<xs:sequence>
				<xs:choice>
					<!-- CNPJ de um contribuinte -->
					<xs:element name="CNPJ" type="TCnpj"/>
					<!-- CPF de um contribuinte -->
					<xs:element name="CPF" type="TCpf"/>
				</xs:choice>
				<!-- Nome ou razao social -->
				<xs:element maxOccurs="1" minOccurs="0" name="xNome" type="TString100"/>
				<!-- Inscricao municipal  -->
				<xs:element maxOccurs="1" minOccurs="0" name="IM" type="TString15"/>
				<!-- Codigo do municipio   -->
				<xs:element maxOccurs="1" minOccurs="0" name="cMun" type="TCodMunIBGE"/>
				<!-- Nome do municipio -->
				<xs:element maxOccurs="1" minOccurs="0" name="xMun" type="TString60"/>
				<!-- UF -->
				<xs:element name="UF" type="TUf"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- Definicao da estrutura de dados para tomador de um CFSe -->
	<xs:complexType name="TomSCupom">
		<xs:sequence>
			<xs:choice>
				<!-- CNPJ de um contribuinte -->
				<xs:element name="CNPJ" type="TCnpj"/>
				<!-- CPF de um contribuinte -->
				<xs:element name="CPF" type="TCpf"/>
			</xs:choice>
			<!-- Nome ou razao social -->
			<xs:element maxOccurs="1" minOccurs="0" name="xNome" type="TString100"/>
			<!-- Endereco de correio eletronico -->
			<xs:element maxOccurs="1" minOccurs="0" name="xEmail" type="TString120"/>
		</xs:sequence>
	</xs:complexType>
	<!-- Definicao da estrutura de dados para endereco de um tomador de uma NFSe -->
