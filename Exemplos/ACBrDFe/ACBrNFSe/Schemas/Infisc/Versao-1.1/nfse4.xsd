				<xs:element maxOccurs="1" minOccurs="1" name="CNPJ" type="TCnpj"/>
				<!-- Chave de acesso -->
				<xs:element maxOccurs="1" minOccurs="1" name="chvAcessoNFS-e" type="TChaveAcesso"/>
