	"definicao":
	{
		"campos": [
			{
				"nome": "data",
				"descricao": "Informar a data oficial do feriado. O formato deve ser dd/mm. Somente deve ser informada para os feriados fixos, pois a data dos feriados móveis é calculada.",
				"tipo": "String",
				"obrigatorio": false
			},
			{
				"nome": "nome",
				"descricao": "Informar o nome do feriado.",
				"tipo": "String",
				"obrigatorio": true
			},
			{
				"nome": "descricao",
				"descricao": "Informar a descrição do feriado.",
				"tipo": "String",
				"obrigatorio": false
			},
			{
				"nome": "tipo",
				"descricao": "Informar o tipo do feriado.",
				"valores": "1-Feriado Nacional, 2-Feriado Estadual, 3-Feriado Municipal, 4-Ponto Facultativo",
				"tipo": "Integer",
				"obrigatorio": true
			},
			{
				"nome": "listaUF",
				"descricao": "Para feriados estaduais ou pontos facultativos, pode-se informar a lista das UF em que o dia se aplica.",
				"tipo": "String",
				"obrigatorio": false
			},
			{
				"nome": "listaCidade",
				"descricao": "Para feriados municipais ou pontos facultativos, pode-se informar a lista das cidades em que o dia se aplica.",
				"tipo": "String",
				"obrigatorio": false
			},
			{
				"nome": "dataInicio",
				"descricao": "Informar a data de início do feriado, p. ex. a data em que o feriado foi decretado.",
				"tipo": "Date",
				"obrigatorio": false
			},
			{
				"nome": "legislacao",
				"descricao": "Informar a lei ou o decreto que instituiu o feriado.",
				"tipo": "String",
				"obrigatorio": false
			},
			{
				"nome": "legislacaoLink",
				"descricao": "Informar o link de acesso à lei ou ao decreto que instituiu o feriado.",
				"tipo": "String",
				"obrigatorio": false
			},
			{
				"nome": "trocaDiaTipo",
				"descricao": "Caso o feriado tenha de ser transferido de dia, pode-se informar o tipo de troca de dia do feriado.",
				"valores": "1-Troca De-Para, 2-De Dia Útil para Segunda ou Sexta",
				"tipo": "Integer",
				"obrigatorio": false
			},
			{
				"nome": "trocaDiaDe",
				"descricao": "Informar a lista dos dias da semana em que, se cair o dia oficial do feriado, ocorrerá a troca.",
				"tipo": "String",
				"obrigatorio": false
			},
			{
				"nome": "trocaDiaPara",
				"descricao": "Informar o dia da semana para o qual será feita a troca.",
				"tipo": "Integer",
				"obrigatorio": false
			},
			{
				"nome": "trocaDiaParaSemana",
				"descricao": "Informar se deve haver troca na semana.",
				"valores": "..., -1- Semana Anterior, 0-Mesma Semana, 1-Semana Seguinte, ...",
				"tipo": "Integer",
				"obrigatorio": false
			}
		]
	},
